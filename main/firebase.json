{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "headers": [
      {
        "source": "**/*",
        "headers": [
          {
            "key": "Cross-Origin-Embedder-Policy",
            "value": "credentialless"
          }
        ]
      }
    ]
  }
}
